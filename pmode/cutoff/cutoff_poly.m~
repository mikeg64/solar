function cutoff=cutoff_poly(pc, z) 

omega=2.2833333378999998e+2/(pc.p1*x.^9+pc.p2*x.^8+pc.p3*x.^7+pc.p4*x.^6+pc.p5*x.^5+pc.p6*x.^4+pc.p7*x.^3+pc.p8*x.^2+pc.p9*x+pc.p10);
omega=sqrt(4.5666666757999997e2.*(1.8535999999999999e3.*z.^7+1.3293e3.*z.^6-8.4e3.*z.^5-3.547e3.*z.^4+12456.*z.^3+5.2979999999999995e2.*z.^2-5066.*z+1434)./(2.3169999999999999e2.*z.^8+1.8990000000000001e2.*z.^7-1.4e3.*z.^6-7.0939999999999998e2.*z.^5+3114.*z.^4+1.7659999999999999e2.*z.^3-2533.*z.^2+1434.*z+1.163e4)+5.2136111319655545e4./(2.3169999999999999e2.*z.^8+1.8990000000000001e2.*z.^7-1.4e3.*z.^6-7.0939999999999998e2.*z.^5+3114.*z.^4+1.7659999999999999e2.*z.^3-2533.*z.^2+1434.*z+1.163e4).^2);
    
    cutoff=2.*pi./omega;